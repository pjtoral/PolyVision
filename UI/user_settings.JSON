{
  "app_name": "Image Acquisition App",
  "version": "1.0.0",
  "camera_settings": {
    "resolution": "high",
    "file_format": "jpeg",
    "capture_delay": 3,
    "exposure": 0.5,
    "white_balance": "auto",
    "flash": "auto",
    "image_stabilization": true
  },
  "ui_settings": {
    "grid_overlay": true,
    "ui_theme": "dark",
    "language": "en"
  },
  "extra_features": {
    "geotagging": false,
    "audio_capture": true,
    "timer_shooting": false,
    "interval_shooting": false
  },
  "image_processing_settings": {
    "resize": {
      "enabled": true,
      "width": 800,
      "height": 600
    },
    "filter": {
      "enabled": true,
      "type": "grayscale"
    }
  }
}
